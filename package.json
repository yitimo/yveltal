{
  "name": "yveltal",
  "version": "2.0.0",
  "description": "Comment backend in nodejs for personal blog.",
  "repository": "git@github.com:yitimo/yveltal.git",
  "author": "yitimo <admin@yitimo.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=egg-zygarde --workers=1",
    "start:d": "egg-scripts start --daemon --title=egg-zygarde --workers=1",
    "stop": "egg-scripts stop --title=egg-zygarde --workers=1",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "lint": "eslint .",
    "autod": "autod"
  },
  "dependencies": {
    "egg": "^2.28.0",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.13.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.15.0",
    "eslint": "^7.10.0",
    "eslint-config-egg": "^8.1.2"
  }
}
